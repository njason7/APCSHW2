public class Recursion{

    public Recursion(){

    }

    public String name(){
	return "Ng,Jason";
    }

    public int fact(int n){
	if (n > 0){
	    return n*(fact n-1);
	}
    }

    public static void main(String[]args){
	Recursion test = new Recursion();
	System.out.println(test.fact(3));
    }
}
